<Window x:Class="Proyecto.Eliminar"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Proyecto"
        mc:Ignorable="d"
        Title="Eliminar" Height="450" Width="400">
    <Grid>
        <ListBox x:Name="listBoxEliminar" HorizontalAlignment="Left" Margin="10,10,0,10" Width="259" ItemsSource="{Binding ItemsSource,  ElementName=listBoxEliminar}"/>
        <Button x:Name="btnEliminar" Content="Button" HorizontalAlignment="Left" Margin="312,179,0,220" Width="75" Click="BtnEliminar_Click"/>

    </Grid>
</Window>
